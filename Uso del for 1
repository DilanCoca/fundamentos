#include <iostream>
using namespace std;
int main()
{
    int n, i, acum=1;
    do{
        cout << " Â¿Hasta que numero desea multiplicar?";
        cin>> n;
    }while(n < 0);
    for (i = 1; i<=n; i++) {
        acum = acum * i;
    }
    cout << "El factorial de "<<n <<"es:" << acum;
    return 0;
}

