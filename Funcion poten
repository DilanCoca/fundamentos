
#include <iostream>
#include <math.h>

using namespace std;

int POTEN(double x, double y);
int main()
{
    double x, y;
    cout << "Ingresar X: ";
    cin >> x;
    
    cout << "Ingresar la potencia Y: ";
    cin >> y;
    
    cout << "El resultado es: " << POTEN(x,y);

    return 0;
}
int POTEN(double x, double y) {
    x = pow(x, y);
    
    return x;
}
