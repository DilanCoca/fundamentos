#include <iostream>

using namespace std;

int poten(int x, int y);
int main()
{
    int x,y;
    cout<<"Ingrese numero: "<<endl;
    cin>>x;
    cout<<"Ingrese cuantas veces desea repetirlo: "<<endl;
    cin>> y;
    cout<<"El resultado de la potencia es: "<<poten(x,y)<<endl;

    return 0;
}

int poten(int x, int y){
    int acum=1;
    for(int i=1;i<=y; i++){
    acum*=x;   
    }
return acum;
}
